{
  "root": "frontend/dist",
  "routes": {
    "/admin": "/index.html",
    "/admin/*": "/index.html",
    "/*": "/index.html"
  },
  "headers": {
    "/*": {
      "Cache-Control": "no-cache"
    },
    "/static/*": {
      "Cache-Control": "public, max-age=31536000"
    }
  },
  "https_only": true,
  "error_page": "index.html"
} 